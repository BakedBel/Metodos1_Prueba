      "source": [
        "import math\n",
        "\n",
        "def snell_angle(n1, n2):\n",
        "    refraction = math.asin((1/n2) * math.sin(1/n1))\n",
        "    incidence = math.sin(1/n1)\n",
        "    return math.degrees(incidence), math.degrees(refraction)\n",
        "\n",
        "\n",
        "\n",
        "print(snell_angle(1,1.33))"
      ]
    }
